pipeline{
    agent any
    options {
        // This is required if you want to clean before build
        skipDefaultCheckout(true)
    }
    stages{
        stage('Cloning'){
            steps{
                sh "git clone https://github.com/devopsPRO27/kub"
            }
        }
        stage('building') {
            steps{
                // Clean before build
                cleanWs()
                sh "npm install"
                sh "npm start"
            }
        }
            
    }
}